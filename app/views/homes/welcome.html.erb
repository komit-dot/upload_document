<h1>Shared Posts</h1>
<div class="container">            
  <table class="table" border="3" width="100%">
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Image</th>
        <th>Tags</th>
        <th>Tag Links</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
<% @posts.each do |post| %>
      <%= render partial: post, locals: {sharing: true}  %>
<% end %>
    </tbody>
  </table>
</div>